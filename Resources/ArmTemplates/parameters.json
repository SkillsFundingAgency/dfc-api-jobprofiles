{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appSharedResourceGroup": {
            "value": "__appSharedResourceGroup__"
        },
        "appSharedStorageAccountName": {
            "value": "__appSharedStorageAccountName__"
        },
        "environment": {
            "value": "__ReleaseEnvironmentName__",
            "metadata": {
                "comment": "Bug in tokenisation task prevents referencing Release.EnvironmentName directly.  https://github.com/TotalALM/VSTS-Tasks/issues/29"
            }
        },
        "cosmosDbName": {
            "value": "__appSharedCosmosDBName__"
        },
        "cosmosDbKey": {
            "value": "__appSharedCosmosPrimaryKey__"
        },
        "sharedAppServicePlanName": {
            "value": "__sharedAppServicePlanName__"
        },
        "sharedAppServicePlanResourceGroup": {
            "value": "__appSharedResourceGroup__"
        },
        "FunctionAppName": {
            "value": "__FunctionAppName__"
        },
        "ApimProductInstanceName": {
            "value": "__ApimProductInstanceName__"
        },
        "ApimResourceGroup": {
            "value": "__SharedResourceGroupName__"
        },
        "ApimServiceName":  {
            "value": "__ApimServiceName__"
        },
        "ApiName": {
            "value": "__ApiName__"
        },
        "ApiVersionNumber": {
            "value": "__ApiVersion__"
        },
        "ApimLoggerName": {
            "value": "__ApimLoggerName__"
        },
        "JobProfileSearchIndexName": {
            "value": "__JpSearchIndexName__"
        },
        "JobProfileSearchServiceName": {
            "value": "__JpSearchServiceName__"
        },
        "JobProfileSearchServiceAccessKey": {
            "value": "__JpSearchServiceAccessKey__"
        },
        "SharedConfigDigitalServiceName": {
            "value": "__SharedConfigDigitalServiceName__"
        },
        "SharedConfigDigitalSearchKeyName": {
            "value": "__SharedConfigDigitalSearchKeyName__"
        },
        "SharedConfigStorageConnectionString": {
            "value": "__SharedConfigStorageConnectionString__"
        },
        "SharedConfigTableName": {
            "value": "__SharedConfigTableName__"
        },
        "SharedConfigTTL": {
            "value": "__SharedConfigTTL__"
        },
        "enableAlerts": {
            "value": __EnableAzureMonitorAlerting__
        },
        "exceptionsThreshold": {
            "value": "__ExceptionsThreshold__"
        },
        "workspaceName": {
            "value": "__sharedLogAnalyticsWorkspace__"
        },
        "workspaceResourceGroup": {
            "value": "__sharedLogAnalyticsResourceGroup__"
        },
        "CmsTokenEndpointUrl": {
            "value": "__StaxGraphqlTokenEndpointUrl__"
        },
        "CmsGraphApiUrl": {
            "value": "__StaxGraphqlApiUrl__"
        },
        "CmsClientId": {
            "value": "__StaxGraphqlClientId__"
        },
        "CmsClientSecret": {
            "value": "__StaxGraphqlClientSecret__"
        },
        "CmsRedisCacheConnectionString": {
            "value": "__RedisConnectionString__"
        },
        "CmsSqlApiUrl": {
            "value": "__StaxGraphqlSqlApiUrl__"
        },
        "CmsCacheKeyExpiry": {
            "value": "__CmsCacheKeyExpiry__"
        }

    }
}